{
    "name": "paulmenheere/laravel-bootstrap-4-forms",
    "type": "package",
    "description": "Bootstrap 4 form builder for Laravel 5 Forked for development speedup",
    "keywords": [
        "laravel",
        "bootstrap",
        "forms",
        "php",
        "form-builder",
        "composer",
        "package"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jos√© Sousa",
            "email": "sputinykster@gmail.com"
        },
        {
            "name": "Paul Menheere",
            "email" : "paul@pm-consultancy.nl"
        }
    ],
    "version": "3.0.0",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5",
        "mockery/mockery": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "NetoJose\\Bootstrap4Forms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NetoJose\\Bootstrap4Forms\\Bootstrap4FormsServiceProvider"
            ],
            "aliases": {
                "Form": "NetoJose\\Bootstrap4Forms\\Bootstrap4FormsFacade"
            }
        }
    }
}
